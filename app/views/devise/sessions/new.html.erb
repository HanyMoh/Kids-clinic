<div class="content-wrap">
  <h6>تسجيل الدخول</h6>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <%= f.text_field :email,autofocus: true,placeholder:"البريد الإلكترونى", class: "form-control" %>
  <%= f.password_field :password, autocomplete: "off",placeholder:"كلمة المرور", class: "form-control" %>

  <% if devise_mapping.rememberable? -%>
  <div class="field">
    <%= f.check_box :remember_me %>
    <%= f.label :remember_me,"تذكرنى" %>
  </div>
  <% end -%>

  <div class="action">
    <%= f.submit "تسجيل الدخول", class: "btn btn-primary signup" %>
  </div>
<% end %>
</div>

  <body class="hold-transition login-page">
    <div class="login-box">
      <div class="login-logo">
        <%= link_to "Ehab kids clinic", root_path %>
      </div>
      <!-- /.login-logo -->
      <div class="login-box-body">
        <!-- <div class="login-box-msg"> -->
        <%= render "layouts/flash" %>
        <%= yield :flash_message %>
